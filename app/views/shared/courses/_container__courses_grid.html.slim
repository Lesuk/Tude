- partial_path = course_partial_path
.container__courses-grid.h-clearfix.container-box
  .courses-grid.container-box
    - courses.each do |course|
      = render partial: partial_path, locals: {object: course, show_progress: true}

  = paginate courses, window: 3, outer_window: 1
  = render partial: "shared/filter_per_page"
