<div class="questionnaires_<%= action_name %>">
  <h1>
      Surveys Index
  </h1>
  <hr>
  <%= link_to "New Survey", new_survey %>

  <ul>
    <% @surveys.each do |survey| %>
      <li>
        <%= link_to survey.name, new_attempt_path(survey_id: survey.id) %>
        <%= link_to 'edit', edit_quiz_path(survey) %>
      </li>
    <% end -%>
  </ul>

</div>
